{
  "name": "myinvoice-be-express",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "dev": "nodemon src/Public/app.ts",
    "vscode-debug": "node --inspect=127.0.0.1:9230 -r ts-node/register src/Public/app.ts",
    "generate": "graphql-codegen --config codegen.yml",
    "migrate": "npx sequelize-cli db:migrate",
    "unmigrate": " npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "refreshdb": "npm run unmigrate && npm run migrate && npm run seed"
  },
  "author": "Awais",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.8",
    "apollo-server-express": "^2.18.2",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.4",
    "sequelize": "^6.3.5",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.17.10",
    "@graphql-codegen/introspection": "1.18.0",
    "@graphql-codegen/typescript": "^1.17.10",
    "@graphql-codegen/typescript-resolvers": "1.17.10",
    "body-parser": "^1.19.0",
    "ts-node": "^9.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm run vscode-debug",
    "ext": "ts"
  }
}
